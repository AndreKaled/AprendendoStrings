public class MudaString {

	public static void main(String[] argumentos) {

		String nome = "Dan Gusfield";
		// nome.charAt(3) = '+';
		// O metodo charAt() é um método que retorna o caractere passado na
		// posição do argumento, não tendo utilidade para
		// alterar a String
		System.out.println(nome);
	}
}
